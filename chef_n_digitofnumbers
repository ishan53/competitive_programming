#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main()
{
	ll t;
	cin>>t;
	while(t--)
	{
	string n;
	cin>>n;
	long long int x,a=0,b=0;
	x=n.length();
	for (int i = 0; i < x; ++i)
	{
		if(n[i]=='0')
			a++;
		else
			b++;
	}
	if(a==1 || b==1)
        cout<<"Yes"<<endl;
    	else
		cout<<"No"<<endl;
	}
	return 0;
}
